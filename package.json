{
  "name": "sheetflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier './**/*.{js,ts,jsx,tsx,mjs,cjs,json}' --write",
    "check": "tsc -b --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mantine/hooks": "^8.2.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/utils": "^7.2.0",
    "@xyflow/react": "^12.8.2",
    "clsx": "^2.1.1",
    "elkjs": "^0.10.0",
    "events": "^3.3.0",
    "hyperformula": "^3.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-inspector": "^6.0.2",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-links": "^9.0.18",
    "@storybook/addon-themes": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.18",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "storybook": "^9.0.18",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "web-worker": "^1.5.0"
  },
  "engines": {
    "node": ">=22.12",
    "pnpm": ">=10"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=22.12"
    },
    "packageManager": {
      "name": "pnpm",
      "onFail": "warn",
      "version": ">=10"
    }
  }
